@model List<Productos>

@{
    ViewData["Title"] = "Reportes Page";
}

<div class="d-flex justify-content-start">

    <div class="form-group me-4">
        <label for="fechaInicio">Fecha de Inicio</label>
        <input type="text" id="fechaInicio" class="form-control datepicker">
    </div>
    <div class="form-group">
        <label for="fechaFin">Fecha de Fin</label>
        <input type="text" id="fechaFin" class="form-control datepicker">
    </div>
</div>

<div class="d-flex justify-content-end">
    <div class="form-group">
        <label for="buscar">Buscar</label>
        <input type="text" id="buscar" class="form-control">
    </div>
</div>



<table class="table">
    <thead>
        <tr>
            <th>Código</th>
            <th>Nombre</th>
            <th>Descripción</th>
            <th>Calidad</th>
            <th>Especificaciones</th>
            <th>Fecha de Vencimiento</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var producto in Model)
        {
            <tr>
                <td>@producto.Codigo</td>
                <td>@producto.Nombre</td>
                <td>@producto.Descripcion</td>
                <td>@producto.Calidad</td>
                <td>@producto.Especificaciones</td>
                <td>@producto.FechaDeVencimiento.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>
